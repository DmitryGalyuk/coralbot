{
  "devDependencies": {
    "exceljs": "^4.4.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "23.0.1"
  },
  "scripts": {
    "test": "node --experimental-vm-modules ./node_modules/.bin/jest"
  },
  "type": "module",
  "jest": {
    "transform": {},
    "verbose": true,
    "testEnvironment": "jest-environment-jsdom",
    "resetMocks": false,
    "roots": [
      "<rootDir>/web",
      "<rootDir>/tests"
    ],
    "moduleNameMapper": {
      "^uuid$": "uuid"
    }
  },
  "directories": {
    "test": "tests"
  }
}
